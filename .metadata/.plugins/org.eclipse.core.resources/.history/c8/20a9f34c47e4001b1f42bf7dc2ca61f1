package projetopp2;
import java.util.List;
public class Projeto {

	 public void start(List<FuncionarioNoTrabalho> funcionarioNoTrabalho) {

	      
			for(FuncionarioNoTrabalho funcionarioNoTrabalho:funcionarioNoTrabalho) {
	            funcionarioNoTrabalho.trabalhos();
	        }
	    }
	
}
