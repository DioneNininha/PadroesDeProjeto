package veiculos;
/**
*
* @author Dione
*
*/
public class Diciclo implements IVeiculo {
private double motor;
private String cor;
private String ano;
@Override
public void configuracaoDiciclo( double motor, String cor, String ano)
{
this. motor = motor;
this. cor = cor;
this. ano = ano;
System. out.println("Criação de um Diciclo: " + motor + " " + cor
+ " " + ano);
startVeiculo();
}
@Override
public void configuracaoQuadriciclo(double motor,
String cor, String ano, int bancos) {
}
@Override
public void startVeiculo() {
}
}